<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- HxC Floppy Emulator Disk Layout -->

<!-- Otrona Attaché  -->

<!-- Note : Ready signal on pin 6. -->

<disk_layout>
	<disk_layout_name>OTRONA_ATTACHE</disk_layout_name>
	<disk_layout_description>Otrona Attaché Floppy Disk (40 Tracks, 2 Sides, 10 sectors/trk, MFM)</disk_layout_description>
	<prefered_file_extension>dsk</prefered_file_extension>
	<interface_mode>GENERIC_SHUGART_DD_FLOPPYMODE</interface_mode>
	<file_size>409600</file_size>
	<layout>
		<number_of_track>40</number_of_track>
		<number_of_side>2</number_of_side>
		<format>IBM_MFM</format>
		<start_sector_id>1</start_sector_id>
		<sector_per_track>10</sector_per_track>
		<sector_size>512</sector_size>
		<interleave>2</interleave>
		<skew_per_side>0</skew_per_side>
		<skew_per_track>0</skew_per_track>
		<formatvalue>0xE5</formatvalue>
		<gap3>30</gap3>
		<bitrate>250000</bitrate>
		<pregap>0</pregap>
		<rpm>300</rpm>
	</layout>
</disk_layout>
